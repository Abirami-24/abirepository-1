
node{
   stage('SCM Checkout'){
     git 'https://github.com/Abirami-24/abirepository-1'
   }
   stage('Compile-Package'){
      // Get maven home path
    def mvn_home = 'maven'
    withEnv( ["PATH+MAVEN=${tool mvn_home}/bin"] ) {
     sh "mvn clean install"
    }
   }
   stage('Deploy to Tomcat'){
      sshagent(['tomactkey']){
      sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/webapp/abi_pipeline/target/*.war  ec2-user@35.174.208.62:apache-tomcat-7.0.104/webapps/'
      }
  }
}
   
  
