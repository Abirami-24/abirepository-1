
node{
   stage('SCM Checkout'){
     git 'https://github.com/Abirami-24/abirepository-1'
   }
   stage('Deploy to Tomcat'){
      sshagent(['tomactkey']){
      sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/webapp/target/*.war  ec2-user@3.88.59.170:tomcat7/webapps/'
      }
  }
}
   
  
